<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width,minimum-scale=1,maximum-scale=1,user-scalable=no">
  </head>
  <style>
    *{
      box-sizing: border-box;
      padding: 0;
      margin: 0;
    }
    .container{
        padding: 20px;
    }
    .container a{
        display: block;
        margin-bottom: 10px;
    }
    ul{
      list-style: none;
    }
    ul li {
      float: left;
      width: 200px;
      height: 100px;
      margin-bottom: 10px;
    }
    ul li~li {
      margin-left: 10px; 
    }
    ul li a {
      width: 100%;
      height: 100px;
    }
    ul li img {
      width: 100%;
      height: 100%;
    }
  </style>
  <body>
    <div class="container">
      <ul>
        <%for(var i = 0;i<list.length;i++){%>
          <li>
            <a href='<%= list[i].url %>'>
              <img src="<%= list[i].screenshot %>"/>
            </a>
            <p><%= list[i].basename %></p>
          </li>
        <%}%>
      </ul>

      </div>
  </body>
</html>